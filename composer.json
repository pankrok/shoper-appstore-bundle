{
    "name": "pankrok/shoper-appstore-bundle",
    "type": "symfony-bundle",
    "version": "1.1.2",
    "description": "This bundle provides Shoper appstore SDK",
    "keywords": ["shoper", "appstore", "api"],
    "license": "MIT",
    "authors": [
        { 
            "name": "Jerzy RÃ³g"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "exercise/htmlpurifier-bundle": "^4.0",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "1.17|^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/runtime": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/yaml": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.34",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PanKrok\\ShoperAppstoreBundle\\": "src/"
        }
        
    },
    "autoload-dev": {
        "psr-4": {
            "PanKrok\\ShoperAppstoreBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
